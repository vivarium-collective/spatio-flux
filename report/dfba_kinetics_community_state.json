        "glucose": [
          "fields",
          "glucose"
        ],
        "acetate": [
          "fields",
          "acetate"
        ]
      },
      "biomass": [
        "fields",
        "dfba biomass"
      ]
    }
  },
  "monod_kinetics": {
    "address": {
      "protocol": "local",
      "data": "MonodKinetics"
    },
    "config": {
      "reactions": {
        "assimilate_glucose": {
          "reactant": "glucose",
          "product": "mass",
          "km": 0.5,
          "vmax": 0.01,
          "yield": 1.0
