      "substrates": {
        "acetate": [
          "fields",
          "acetate"
        ]
      },
      "biomass": [
        "fields",
        "kinetic biomass"
      ]
    },
    "outputs": {
      "substrates": {
        "acetate": [
          "fields",
          "acetate"
        ]
      },
      "biomass": [
        "fields",
        "kinetic biomass"
      ]
    }
  },
  "fields": {
    "glucose": 10.0,
    "acetate": 0.0,
    "dfba biomass": 0.01,
    "kinetic biomass": 0.01
  },
  "emitter": {
    "address": {
      "protocol": "local",
      "data": "RAMEmitter"
