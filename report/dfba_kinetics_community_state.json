{
  "global_time": 0.0,
  "dFBA": {
    "address": {
      "protocol": "local",
      "data": "DynamicFBA"
    },
    "config": {
      "bounds": {
        "ATPM": {
          "lower": 1.0,
          "upper": 1.0
        },
        "EX_o2_e": {
          "lower": -2.0
        }
      },
      "kinetic_params": {
        "acetate": [
          0.5,
          2.0
